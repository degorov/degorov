{
    "breadcrumbs.enabled": false,
    "debug.javascript.codelens.npmScripts": "never",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "Cascadia Mono",
    "editor.formatOnSave": true,
    "editor.guides.highlightActiveIndentation": false,
    "editor.lineHeight": 22,
    "editor.links": false,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderLineHighlight": "all",
    "editor.roundedSelection": false,
    "editor.rulers": [120],
    "editor.selectionHighlight": false,
    "editor.smoothScrolling": true,
    "editor.tabSize": 4,
    "explorer.compactFolders": false,
    "explorer.decorations.colors": false,
    "explorer.incrementalNaming": "smart",
    "files.autoGuessEncoding": true,
    "files.enableTrash": false,
    "files.eol": "\n",
    "files.hotExit": "onExitAndWindowClose",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "prettier.requireConfig": true,
    "rest-client.enableTelemetry": false,
    "scm.diffDecorationsIgnoreTrimWhitespace": "inherit",
    "search.showLineNumbers": true,
    "selectline.alignment": "right",
    "selectline.displayFormat": "%d lines selected",
    "selectline.statusbarPriority": 101,
    "telemetry.telemetryLevel": "off",
    "terminal.explorerKind": "external",
    "terminal.external.windowsExec": "wt",
    "terminal.integrated.automationProfile.windows": {
        "icon": "terminal-cmd",
        "path": "C:\\Windows\\System32\\cmd.exe"
    },
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontWeightBold": "normal",
    "window.newWindowDimensions": "maximized",
    "window.title": "${dirty}${rootName}${separator}${activeEditorMedium}",
    "window.titleBarStyle": "custom",
    "workbench.activityBar.iconClickBehavior": "focus",
    "workbench.colorCustomizations": {
        "[Default Light Modern]": {
            "editor.background": "#F8F8F8",
            "editorIndentGuide.background1": "#EEEEEE"
        }
    },
    "workbench.colorTheme": "Default Light Modern",
    "workbench.editor.decorations.colors": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "file-icons",
    "workbench.list.smoothScrolling": true,
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false,
    "workbench.tree.renderIndentGuides": "none"
}
